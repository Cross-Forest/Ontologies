###  http://crossforest.eu/ifn/ontology/hasDBH1
ifn:hasDBH1 rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ifn:hasDiameterAtBreastHeight ;
            rdfs:domain ifn:Tree ;
            rdfs:range ifn:DBH1 ;
            rdfs:comment "has diameter at breast hight with the caliper arms pointing at the center of the plot"@en ,
                         "tiene diámetro normal con la forcípula apuntando al centro de la parcela"@es ,
                         "tiene diámetro observado a la altura del pecho con la forcípula apuntando al centro de la parcela"@es ;
            rdfs:isDefinedBy <http://crossforest.eu/ifn/ontology/> ;
            rdfs:label "has DBH1"@en ,
                       "tiene dn1"@es .


###  http://crossforest.eu/ifn/ontology/hasDBH2
ifn:hasDBH2 rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ifn:hasDiameterAtBreastHeight ;
            rdfs:domain ifn:Tree ;
            rdfs:range ifn:DBH2 ;
            rdfs:comment "has diameter at breast hight with the caliper arms perpendicular to the center of the plot"@en ,
                         "tiene diámetro normal con la forcípula perpendicular al centro de la parcela"@es ,
                         "tiene diámetro observado a la altura del pecho con la forcípula perpendicular al centro de la parcela"@es ;
            rdfs:isDefinedBy <http://crossforest.eu/ifn/ontology/> ;
            rdfs:label "has DBH2"@en ,
                       "tiene dn2"@es .


###  http://crossforest.eu/ifn/ontology/hasDiameterAtBreastHeight
ifn:hasDiameterAtBreastHeight rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf smo:hasMeasure ;
                              rdfs:domain ifn:Tree ;
                              rdfs:range ifn:DBH ;
                              rdfs:comment "has diameter at breast hight"@en ,
                                           "tiene diámetro a la altura del pecho"@es ,
                                           "tiene diámetro normal"@es ;
                              rdfs:isDefinedBy <http://crossforest.eu/ifn/ontology/> ;
                              rdfs:label "has DBH"@en ,
                                         "tiene DN"@es .


###  http://crossforest.eu/ifn/ontology/hasForestArea
