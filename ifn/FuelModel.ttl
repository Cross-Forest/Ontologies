@prefix ifn: <http://crossforest.eu/ifn/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.w3.org/2002/07/owl#> .

###  http://crossforest.eu/ifn/ontology/ModeloCombustible
ifn:ModeloCombustible rdf:type owl:Class .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible01
ifn:ModeloCombustible01 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Pastos> ;
                        rdfs:comment "Pasto fino, seco y bajo, que recubre completamente el suelo. Pueden aparecer algunas plantas leñosas dispersas ocupando menos de 1/3 de la superficie."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Pasto fino con leñosas dispersas"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible02
ifn:ModeloCombustible02 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Pastos> ;
                        rdfs:comment "Pasto fino, seco y bajo, que recubre completamente el suelo. Las plantas leñosas dispersas cubren de 1/3 a 2/3 de la superficie, pero la propagación del fuego se realiza por el pasto."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Pasto fino con leñosas"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible03
ifn:ModeloCombustible03 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Pastos> ;
                        rdfs:comment "Pasto grueso, denso, seco y alto ( > 1m). Puede haber algunas plantas leñosas dispersas. Los campos de cereales son representativos de este modelo."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Pasto grueso"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible04
ifn:ModeloCombustible04 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral> ;
                        rdfs:comment "Matorral o plantación joven muy densa; de más de 2m. de altura; con ramas muertas en su interior. Propagación del fuego por las copas de las plantas."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Matorral Joven"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible05
ifn:ModeloCombustible05 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral> ;
                        rdfs:comment "Matorral disperso, denso y verde, de menos de 1m. de altura. Propagación del fuego por la hojarasca, el pasto, las ramillas y el matorral"@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Matorral disperso"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible06
ifn:ModeloCombustible06 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral> ;
                        rdfs:comment "Parecido al modelo 5, pero con especies más inflamables, de mayor talla, pudiéndose encontrar ramas gruesas en el suelo. Propagación del fuego con vientos moderados a fuertes."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Matorral disperso inflamable"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible07
ifn:ModeloCombustible07 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral> ;
                        rdfs:comment "Matorral de especies muy inflamables; de 0,5 a 2m. de altura, situado como sotobosque en masas de coníferas."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Matorral muy inflamable"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible08
ifn:ModeloCombustible08 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_HojarascaBajoArbolado> ;
                        rdfs:comment "Bosque denso, sin matorral. Propagación del fuego por la hojarasca muy compacta, formada por acículas cortas (5 cm o menos) o por hojas planas no muy grandes."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Bosque de hojarasca compacta"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible09
ifn:ModeloCombustible09 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_HojarascaBajoArbolado> ;
                        rdfs:comment "Parecido al modelo 8, pero con hojarasca menos compacta, formada por acículas largas y rígidas (P. pinaster) o follaje de frondosas de hojas grandes y rizadas (castaño o robles)."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Bosque de hojarasca no compacta"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible10
ifn:ModeloCombustible10 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_HojarascaBajoArbolado> ;
                        rdfs:comment "Bosque con gran cantidad de leña y árboles caídos, como consecuencia de vendavales, plagas intensas, etc."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Bosque con leña y árboles caídos"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible11
ifn:ModeloCombustible11 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_RestosDeCortaYOperacionesSelvicolas> ;
                        rdfs:comment "Bosque claro y fuertemente aclarado. Restos de poda o aclarado ligeros (diámetro <7,5 cm) Restos de poda o aclareo dispersos, con plantas herbáceas rebrotando. La hojarasca y el matorral presente ayudarán a la propagación del fuego."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Bosque claro"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible12
ifn:ModeloCombustible12 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_RestosDeCortaYOperacionesSelvicolas> ;
                        rdfs:comment "Predominio de los restos sobre el arbolado. Restos de poda o aclareo cubriendo todo el suelo,más pesados que los del mod.11"@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Restos sobre el arbolado"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible13
ifn:ModeloCombustible13 rdf:type owl:Class ;
                        rdfs:subClassOf <http://crossforest.eu/ifn/ontology/ModeloCombustible_RestosDeCortaYOperacionesSelvicolas> ;
                        rdfs:comment "Grandes acumulaciones de restos gruesos y pesados,cubriendo todo el suelo."@es ;
                        rdfs:isDefinedBy ifn: ;
                        rdfs:label "Restos gruesos y pesados"@es .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible_HojarascaBajoArbolado
<http://crossforest.eu/ifn/ontology/ModeloCombustible_HojarascaBajoArbolado> rdf:type owl:Class ;
                                                                                   rdfs:subClassOf ifn:ModeloCombustible .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral
<http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral> rdf:type owl:Class ;
                                                                      rdfs:subClassOf ifn:ModeloCombustible .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible_Pastos
<http://crossforest.eu/ifn/ontology/ModeloCombustible_Pastos> rdf:type owl:Class ;
                                                                    rdfs:subClassOf ifn:ModeloCombustible .


###  http://crossforest.eu/ifn/ontology/ModeloCombustible_RestosDeCortaYOperacionesSelvicolas
<http://crossforest.eu/ifn/ontology/ModeloCombustible_RestosDeCortaYOperacionesSelvicolas> rdf:type owl:Class ;
                                                                                                 rdfs:subClassOf ifn:ModeloCombustible .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( ifn:ModeloCombustible01
                ifn:ModeloCombustible02
                ifn:ModeloCombustible03
                ifn:ModeloCombustible04
                ifn:ModeloCombustible05
                ifn:ModeloCombustible06
                ifn:ModeloCombustible07
                ifn:ModeloCombustible08
                ifn:ModeloCombustible09
                ifn:ModeloCombustible10
                ifn:ModeloCombustible11
                ifn:ModeloCombustible12
                ifn:ModeloCombustible13
                <http://crossforest.eu/ifn/ontology/ModeloCombustible_HojarascaBajoArbolado>
                <http://crossforest.eu/ifn/ontology/ModeloCombustible_Matorral>
                <http://crossforest.eu/ifn/ontology/ModeloCombustible_Pastos>
                <http://crossforest.eu/ifn/ontology/ModeloCombustible_RestosDeCortaYOperacionesSelvicolas>
              )
] .