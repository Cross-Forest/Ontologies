@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix iter:  <http://w3id.org/sparql-generate/iter/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mfe:   <http://crossforest.eu/mfe/ontology/> .

mfe:ForestTypeM # modificado x ACO; FormacionArbolada x ForestType
        a           owl:Class , rdfs:Class ;
        rdfs:subClassOf mfe:ForestType ;
        rdfs:label  "Mezcla de coníferas y frondosas"@es .

mfe:ForestTypeC
        a           owl:Class , rdfs:Class ;
        rdfs:subClassOf mfe:ForestType ;
        rdfs:label  "Conifer Stand"@en , "Bosque de coníferas"@es .

mfe:ForestTypeF
        a           owl:Class , rdfs:Class ;
        rdfs:subClassOf mfe:ForestType ;
        rdfs:label  "Frondosas"@es , "Bosques de frondosas"@es .
